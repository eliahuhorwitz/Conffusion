{
    "name": "extract-bounds-conffusion",
    "phase": "train", // train or val
    "gpu_ids": [
        0
    ],
    "path": { //set the path
        "log": "logs",
        "tb_logger": "tb_logger",
        "results": "results",
        "checkpoint": "checkpoint",
         "resume_state": "checkpoints/I640000_E37/I640000_E37" //pretrain model or training state
    },
    "datasets": {
        "calibration": {
            "name": "Conffusion",
            "mode": "LRHR",
            "dataroot": "../datasets/celebahq_16_128/calibration",
            "datatype": "img",
            "num_workers": 1,
            "batch_size": 1,
            "l_resolution": 16,
            "r_resolution": 128,
            "data_len": 1
        },
        "validation": {
            "name": "Conffusion",
            "mode": "LRHR",
            "dataroot": "../datasets/celebahq_16_128/validation",
            "datatype": "img",
             "num_workers": 1,
            "batch_size": 1,
            "l_resolution": 16,
            "r_resolution": 128,
            "data_len": 1
        },
        "test": {
            "name": "Conffusion",
            "mode": "LRHR",
            "dataroot": "../datasets/celebahq_16_128/test",
            "datatype": "img",
            "num_workers": 1,
            "batch_size": 1,
            "l_resolution": 16,
            "r_resolution": 128,
            "data_len": 1
        }
    },
    "model": {
        "which_model_G": "sr3", // use the ddpm or sr3 network structure
        "finetune_norm": false,
        "unet": {
            "in_channel": 6,
            "out_channel": 3,
            "inner_channel": 64,
            "channel_multiplier": [
                1,
                2,
                4,
                8,
                8
            ],
            "attn_res": [
                16
            ],
            "res_blocks": 2,
            "dropout": 0.2,
            "finetune_bounds": false
        },
        "beta_schedule": { // use munual beta_schedule for acceleration
            "train": {
                "schedule": "linear",
                "n_timestep": 2000,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            },
            "val": {
                "schedule": "cosine",
                "n_timestep": 200,
                "linear_start": 1e-6,
                "linear_end": 1e-2
            }
        },
        "diffusion": {
            "image_size": 128,
            "channels": 3, //sample channel
            "conditional": true // unconditional generation or unconditional generation(super_resolution)
        }
    },
    "train": {
        "n_iter": 1000000,
        "val_freq": 1e4,
        "save_checkpoint_freq": 1e4,
        "print_freq": 200,
        "optimizer": {
            "type": "adam",
            "lr": 1e-4
        },
        "ema_scheduler": { // not used now
            "step_start_ema": 5000,
            "update_ema_every": 1,
            "ema_decay": 0.9999
        }
    },
    "wandb": {
        "project": "Conffusion"
    }
}